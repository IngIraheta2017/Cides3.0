{% extends 'base.html.twig' %}

{% block body %}
    <h1>Rol</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ rol.id }}</td>
            </tr>
            <tr>
                <th>Nombrerol</th>
                <td>{{ rol.nombreRol }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{% if rol.createdAt %}{{ rol.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{% if rol.updatedAt %}{{ rol.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('rol_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('rol_edit', { 'id': rol.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
