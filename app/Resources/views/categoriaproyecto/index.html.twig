                <td><a href="{{ path('categoriaproyecto_show', { 'id': categoriaProyecto.id }) }}">{{ categoriaProyecto.id }}</a></td>
                <td>{{ categoriaProyecto.nombreCategoria }}</td>
                <td>{{ categoriaProyecto.descripcionCategoria }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('categoriaproyecto_show', { 'id': categoriaProyecto.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('categoriaproyecto_edit', { 'id': categoriaProyecto.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('categoriaproyecto_new') }}">Create a new categoriaProyecto</a>
        </li>
    </ul>
{% endblock %}
