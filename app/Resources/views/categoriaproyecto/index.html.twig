{% extends 'base.html.twig' %}

{% block body %}
    <h1>Categoriaproyectos list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombrecategoria</th>
                <th>Descripcioncategoria</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for categoriaProyecto in categoriaProyectos %}
